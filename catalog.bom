brooklyn.catalog:
  description: Consul by Hashicorp. Service discovery and configuration made easy
  version: 0.1.0
  publish:
    overview: README.md
    license_url: LICENSE.md
  brooklyn.libraries:
  - https://github.com/cloudsoft/consul/releases/download/0.2.dev/consul-1.2.0-SNAPSHOT.jar
  items:
  - id: consul-node
    item:
      type: classpath://consul/consul-node.yaml
  - id: consul-cluster
    item:
      type: classpath://consul/consul-cluster.yaml
